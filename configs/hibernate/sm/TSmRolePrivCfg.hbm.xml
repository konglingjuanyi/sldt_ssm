<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sldt.mds.dmc.sm.entity.TSmRolePrivCfg"
		table="T_SM_ROLE_PRIV_CFG" lazy="true" batch-size="20">
		<id name="id" type="java.lang.String">
			<column name="ID" length="64" />
			<generator class="assigned" />
		</id>
		<property name="roleId" type="java.lang.String">
			<column name="ROLE_ID" length="32" />
		</property>
		<property name="privId" type="java.lang.String">
			<column name="PRIV_ID" length="64" />
		</property>
		<property name="privType" type="integer">
			<column name="PRIV_TYPE" />
		</property>
	</class>
</hibernate-mapping>