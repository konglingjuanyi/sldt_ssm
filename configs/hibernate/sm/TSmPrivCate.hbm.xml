<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sldt.mds.dmc.sm.entity.TSmPrivCate"
		table="T_SM_PRIV_CATE" lazy="true" batch-size="20">
		<id name="privCateId" type="java.lang.String">
			<column name="PRIV_CATE_ID" length="32" />
			<generator class="assigned" />
		</id>
		<property name="privCateName" type="java.lang.String">
			<column name="PRIV_CATE_NAME" length="240" />
		</property>
		<property name="checkType" type="java.lang.String">
			<column name="CHECK_TYPE" length="1" />
		</property>
	</class>
</hibernate-mapping>